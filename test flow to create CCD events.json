    {
        "id": "2ebf884f941f6ed8",
        "type": "function",
        "z": "95e8fcb9c4a5900c",
        "name": "function 2",
        "func": "// Get previous value (default 0 if none stored yet)\nlet encoderValue = flow.get('encoderValue') || 0;\n\n// Increase by 1\nencoderValue++;\n\n// Save updated value back to flow context\nflow.set('encoderValue', encoderValue);\n\n// Create readable UTC timestamp\nlet now = new Date();\nlet timestamp = now.toISOString().replace('T', ' ').replace('Z', ' UTC');\n\n// Build payload\nmsg.payload = {\n    encoderValue: encoderValue,\n    result: \"OK\",\n    NOKReason: \"000\",\n    timestamp: timestamp,\n    CCDsource: \"CCD1\"\n};\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 360,
        "y": 80,
        "wires": [
            [
                "3ca9338f0aa4a5b4"
            ]
        ]
    },
    {
        "id": "sq",
        "type": "sqlite",
        "z": "95e8fcb9c4a5900c",
        "mydb": "1e915358237a9ac0",
        "sqlquery": "prepared",
        "sql": "SELECT encoderValue, result, NOKReason, timestamp,CCDsource FROM tblCCD1events WHERE (CCDsource = \"CCD1\") ORDER BY encoderValue ASC;",
        "name": "Read CCD1 Events",
