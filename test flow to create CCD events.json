    {
        "id": "transform",
        "type": "function",
        "z": "95e8fcb9c4a5900c",
        "name": "Build Chart Data",
        "func": "let rows = msg.payload;\n\nif (!rows || rows.length === 0) {\n    return null;\n}\n\n// Create clean data array\nlet chartData = [];\n\nrows.forEach(row => {\n    chartData.push({\n        x: Number(row.encodervalue),  // Ensure it's a number\n        y: 0.5,\n        series: String(row.result)    // Ensure it's a string\n    });\n});\n\nnode.warn(\"Total rows: \" + chartData.length);\nnode.warn(\"X min: \" + Math.min(...chartData.map(d => d.x)));\nnode.warn(\"X max: \" + Math.max(...chartData.map(d => d.x)));\n\nmsg.payload = chartData;\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 850,
        "y": 120,
        "wires": [
            [
                "chart"
            ]
